# ===========================================
# Long Tail AI Studio - Environment Variables
# ===========================================
# Copy this to .env.local and fill in your values
# Run: npm run setup (after filling in required values)

# ===========================================
# REQUIRED - Fill these in first
# ===========================================

# Supabase (from supabase.com dashboard > Settings > API)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJ...
SUPABASE_SERVICE_ROLE_KEY=eyJ...

# Stripe (from dashboard.stripe.com/apikeys)
STRIPE_SECRET_KEY=sk_live_...
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_...

# ElevenLabs (from elevenlabs.io/app/settings/api-keys)
ELEVENLABS_API_KEY=...

# ===========================================
# SETUP SCRIPTS - For automated configuration
# ===========================================

# Vercel (from vercel.com/account/tokens)
# Used by: npm run setup:vercel
VERCEL_TOKEN=
VERCEL_PROJECT_ID=
VERCEL_TEAM_ID=

# ===========================================
# AUTO-GENERATED - Run setup scripts
# ===========================================

# Stripe Webhook Secret (generated by setup:stripe)
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Price IDs (generated by setup:stripe)
NEXT_PUBLIC_STRIPE_PRICE_STARTER=price_...
NEXT_PUBLIC_STRIPE_PRICE_BUILDER=price_...
NEXT_PUBLIC_STRIPE_PRICE_SCALE=price_...
NEXT_PUBLIC_STRIPE_CREDITS_50=price_...
NEXT_PUBLIC_STRIPE_CREDITS_150=price_...
NEXT_PUBLIC_STRIPE_CREDITS_500=price_...

# ElevenLabs Agent IDs (generated by setup:elevenlabs)
NEXT_PUBLIC_ELEVENLABS_AGENT_ALEX=
NEXT_PUBLIC_ELEVENLABS_AGENT_SCOUT=
NEXT_PUBLIC_ELEVENLABS_AGENT_MORGAN=
NEXT_PUBLIC_ELEVENLABS_AGENT_VICTORIA=

# ===========================================
# SITE CONFIGURATION
# ===========================================

# Your production URL (update for your domain)
NEXT_PUBLIC_SITE_URL=https://corporateaisolutions.com

# Contact info
NEXT_PUBLIC_CONTACT_EMAIL=hello@corporateaisolutions.com
NEXT_PUBLIC_CONTACT_PHONE=+61402612471

# Skool community
NEXT_PUBLIC_SKOOL_URL=https://www.skool.com/the-easily-distracted-5598
